                    android:layout_width="58dp"
                    android:layout_height="wrap_content"
                    android:checked="false"
                    android:text="1" />

                <CheckBox
                    android:id="@+id/checkBox2"
                    android:layout_width="61dp"
                    android:layout_height="wrap_content"
                    android:text="2" />

                <CheckBox
                    android:id="@+id/checkBox3"
